---
source: native-fungible/tests/format.rs
expression: "NativeFungibleApplication::formats().unwrap()"
---
REGISTRY:
  Account:
    STRUCT:
      - chain_id:
          TYPENAME: ChainId
      - owner:
          TYPENAME: AccountOwner
  AccountEntry:
    STRUCT:
      - key:
          TYPENAME: AccountOwner
      - value:
          TYPENAME: Amount
  AccountOwner:
    ENUM:
      0:
        Reserved:
          NEWTYPE: U8
      1:
        Address32:
          NEWTYPE:
            TYPENAME: CryptoHash
      2:
        Address20:
          NEWTYPE:
            TUPLEARRAY:
              CONTENT: U8
              SIZE: 20
  Amount:
    NEWTYPESTRUCT: U128
  ChainId:
    NEWTYPESTRUCT:
      TYPENAME: CryptoHash
  CryptoHash:
    NEWTYPESTRUCT:
      TUPLEARRAY:
        CONTENT: U8
        SIZE: 32
  FungibleResponse:
    ENUM:
      0:
        Ok: UNIT
      1:
        Balance:
          NEWTYPE:
            TYPENAME: Amount
      2:
        TickerSymbol:
          NEWTYPE: STR
  Message:
    ENUM:
      0:
        Notify: UNIT
  NativeFungibleOperation:
    ENUM:
      0:
        Balance:
          STRUCT:
            - owner:
                TYPENAME: AccountOwner
      1:
        TickerSymbol: UNIT
      2:
        Transfer:
          STRUCT:
            - owner:
                TYPENAME: AccountOwner
            - amount:
                TYPENAME: Amount
            - target_account:
                TYPENAME: Account
      3:
        Claim:
          STRUCT:
            - source_account:
                TYPENAME: Account
            - amount:
                TYPENAME: Amount
            - target_account:
                TYPENAME: Account
OPERATION:
  TYPENAME: NativeFungibleOperation
RESPONSE:
  TYPENAME: FungibleResponse
MESSAGE:
  TYPENAME: Message
EVENT_VALUE: UNIT
