          - key:
              TYPENAME: Key
    1:
      Comment:
        STRUCT:
          - key:
              TYPENAME: Key
          - comment: STR
Operation:
  ENUM:
    0:
      Subscribe:
        STRUCT:
          - chain_id:
              TYPENAME: ChainId
    1:
      Unsubscribe:
        STRUCT:
          - chain_id:
              TYPENAME: ChainId
    2:
      Post:
        STRUCT:
          - text: STR
          - image_url:
              OPTION: STR
    3:
      Like:
        STRUCT:
          - key:
              TYPENAME: Key
    4:
      Comment:
        STRUCT:
          - key:
              TYPENAME: Key
          - comment: STR
OwnPost:
